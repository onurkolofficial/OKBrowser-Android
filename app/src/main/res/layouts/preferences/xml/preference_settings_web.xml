<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:key="settingGeoLocation"
        android:title="@string/geo_location_text"
        app:icon="@drawable/ic_icon_geo_location_circle"/>
    <CheckBoxPreference
        android:key="settingJavascript"
        android:title="@string/javascript_text"
        app:icon="@drawable/ic_icon_javascript_circle"/>
    <CheckBoxPreference
        android:key="settingPopups"
        android:title="@string/popups_text"
        app:icon="@drawable/ic_icon_popups_circle"/>
    <CheckBoxPreference
        android:key="settingSaveForms"
        android:title="@string/save_forms_text"
        app:icon="@drawable/ic_icon_form_circle"/>
    <CheckBoxPreference
        android:key="settingZoom"
        android:title="@string/zoom_support_text"
        app:icon="@drawable/ic_icon_zoom_in_circle"/>
    <CheckBoxPreference
        android:key="settingZoomButtons"
        android:title="@string/zoom_buttons_text"
        android:enabled="false"
        app:icon="@drawable/ic_icon_button_circle"/>
    <CheckBoxPreference
        android:key="settingWebStorage"
        android:title="@string/web_storage_text"
        app:icon="@drawable/ic_icon_storage_circle"/>
    <CheckBoxPreference
        android:key="settingAppCache"
        android:title="@string/app_cache_text"
        app:icon="@drawable/ic_icon_settings_circle"/>
</PreferenceScreen>